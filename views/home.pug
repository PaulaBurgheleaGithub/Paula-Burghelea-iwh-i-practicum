doctype html
html 
    head 
        title=`${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body
        nav
            div
                h1 HubSpot I: Foundations Final Project

            input(
                type='text'
                name='search'
                placeholder='search project...'
            ) 
            ul
                li
                    a(href='/') Home
                li
                    a(href='/update-cobj') Update
        table
            tr.card
                th NAME
                th TYPE
                th STACK
                th DESCRIPTION
            each entry in data
                tr
                    th #{entry.properties.name}
                    th #{entry.properties.type}
                    th #{entry.properties.stack}
                    th #{entry.properties.description}