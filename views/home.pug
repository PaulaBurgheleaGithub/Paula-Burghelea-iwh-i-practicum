doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 HubSpot I: Foundations Final Project
        table
            tr.card
                th NAME
                th TYPE
                th STACK
                th DESCRIPTION
            each entry in data
                tr
                    th #{entry.properties.name}
                    th #{entry.properties.type}
                    th #{entry.properties.stack}
                    th #{entry.properties.description}